{
  "name": "point-of-sales-api",
  "version": "1.0.0",
  "description": "RESTful API for Point of Sales App",
  "main": "server.js",
  "repository": "https://github.com/sutanlab/point-of-sales-api.git",
  "author": "Sutan Gading Fadhillah Nasution",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "nodemon -r dotenv/config server.js",
    "db:init": "sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:rollback": "sequelize db:migrate:undo:all",
    "db:drop": "sequelize db:drop",
    "postinstall": "[ -e .env ] || cp .env.example .env"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.5",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "redis": "^2.8.0",
    "sequelize": "^5.19.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "nodemon": "^1.19.2"
  }
}
